{
  "name": "workflowy-theme-generator",
  "version": "0.0.1",
  "description": "A Theme Generator For Workflowy",
  "scripts": {
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "clean": "del-cli preview",
    "build": "npm-run-all clean build:*"
  },
  "keywords": [
    "workflowy"
  ],
  "author": "Age Poortinga",
  "license": "ISC",
  "devDependencies": {
    "auto-changelog": "^1.4.2",
    "cssnext": "^1.8.4",
    "del-cli": "^1.1.0",
    "npm-run-all": "^4.1.2",
    "postcss": "^6.0.19",
    "postcss-cli": "^5.0.0",
    "postcss-import": "^11.1.0",
    "watch": "^1.0.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "postcss": {
    "map": false,
    "from": "path/to/src/file.css",
    "to": "path/to/dest/file.css"
  }
}
